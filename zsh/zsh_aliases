# Altering default flags
alias cp='cp -i'
alias grep='grep --color'
alias mv='mv -i'
alias rm='rm -i'

# Change Directories
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'

alias abroad="cd ~/Dropbox/Documents/brown/abroad/"
alias db="cd ~/Dropbox"
alias dl="cd ~/Downloads"
alias sb="cd ~/dev/sb"

# Actions
alias ff="find . | xargs grep"
alias fx="firefox"
alias g="git"
alias m="make -j12"
alias suspend="sudo pm-suspend && xlock"
alias xl="xlock"
alias vocab="vim ~/Dropbox/Documents/vocab.txt"

# Android
alias minam='ANDROID_SERIAL=0146BF540F01801A'
alias kaggeum='ANDROID_SERIAL=BCOKAS110839'

# Fennec
alias f_push="adb install dist/fennec*.apk"
alias f_cpush="adb uninstall org.mozilla.fennec_mcomella && \
               adb install dist/fennec*.apk"
alias f_pbuild="rm -rf mobile/android/base && make -C mobile/android && make package" # p = Partial
alias f_cpbuild="f_pbuild && f_cpush"

alias f_bs="adb push ../mobile/android/chrome/content /sdcard/bootstrapper/content/"

alias f_log="adb logcat -v time"

# Temp
alias team="cd $HOME/Documents/course/cs168/team"
